{
    "name": "softspring/time-ago-bundle",
    "description": "This bundle provides a simple twig extension to human readable date diff based on translations",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "symfony/config": "^5.4|^6.0|^7.0@dev",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0@dev",
        "symfony/http-kernel": "^5.4|^6.0|^7.0@dev",
        "symfony/translation": "^5.4|^6.0|^7.0@dev",
        "twig/twig": "*"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\TimeAgoBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev",
            "dev-5.2": "5.2.x-dev"
        }
    }
}
