{
    "name": "softspring/time-ago-bundle",
    "description": "This bundle provides a simple twig extension to human readable date diff based on translations",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "twig/twig": "*",
        "friendsofphp/php-cs-fixer": "9999999-dev"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\TimeAgoBundle\\": "src/"
        }
    },
    "extra": {
    }
}
